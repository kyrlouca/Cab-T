  object TableSQL: TIBCQuery
    SQLInsert.Strings = (
      'INSERT INTO PORT'
      
        '  (DESCRIPTION, GREEK_DESCR, SHORT_GREEK_DESCR, CODE, IS_DEFAULT' +
        '_PORT, IS_DEFAULT_STATION)'
      'VALUES'
      
        '  (:DESCRIPTION, :GREEK_DESCR, :SHORT_GREEK_DESCR, :CODE, :IS_DE' +
        'FAULT_PORT, :IS_DEFAULT_STATION)')
    SQLDelete.Strings = (
      'DELETE FROM PORT'
      'WHERE'
      '  CODE = :Old_CODE')
    SQLUpdate.Strings = (
      'UPDATE PORT'
      'SET'
      
        '  DESCRIPTION = :DESCRIPTION, GREEK_DESCR = :GREEK_DESCR, SHORT_' +
        'GREEK_DESCR = :SHORT_GREEK_DESCR, CODE = :CODE, IS_DEFAULT_PORT ' +
        '= :IS_DEFAULT_PORT, IS_DEFAULT_STATION = :IS_DEFAULT_STATION'
      'WHERE'
      '  CODE = :Old_CODE')
    SQLRefresh.Strings = (
      
        'SELECT DESCRIPTION, GREEK_DESCR, SHORT_GREEK_DESCR, CODE, IS_DEF' +
        'AULT_PORT, IS_DEFAULT_STATION FROM PORT'
      'WHERE'
      '  CODE = :CODE')
    SQLLock.Strings = (
      'SELECT NULL FROM PORT'
      'WHERE'
      'CODE = :Old_CODE'
      'FOR UPDATE WITH LOCK')
    SQLRecCount.Strings = (
      'SELECT COUNT(*) FROM ('
      'SELECT 1 AS C  FROM PORT'
      ''
      ') q')
    Connection = ClairDML.CabCOnnection
    Transaction = ReadTrans
    UpdateTransaction = WriteTrans
    SQL.Strings = (
      'SELECT'
      '*'
